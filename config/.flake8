[flake8]
max-line-length = 248
extend-ignore =
    E203,
    W503,
    E501,
    E722,
    E402,
    F821,
    F523,
    E711,
    E741,
    F401,
    E265,
    C901,
    E301,
    E305,
    W293,
    E261,
    W291,
    W292,
    E111,
    E117,
    F841,
    E302
exclude =
    .git,
    __pycache__,
    build,
    dist,
    .venv,
    venv,
    .eggs,
    *.egg
max-complexity = 30
docstring-convention = google